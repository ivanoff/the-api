{
  "name": "the-api",
  "version": "14.0.1",
  "description": "The API - build your own API",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "node example.js",
    "test": "ts-mocha test/first/*.ts test/*.ts src/routes/**/test/*.ts test/standalone/*.ts test/last/*.ts",
    "coverage": "nyc ts-mocha -- test/first/*.ts test/*.ts src/routes/**/test/*.ts test/standalone/*.ts test/last/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src/ test/ --ignore-pattern .gitignore",
    "fix": "eslint src/ test/ --ignore-pattern .gitignore --fix",
    "migrate:make": "knex migrate:make --migrations-directory=migrations",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@koa/router": "^12.0.0",
    "@types/passport-discord": "^0.1.6",
    "amqplib": "^0.10.1",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "flattening": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-passport": "^5.0.0",
    "koa2-formidable": "^1.0.3",
    "mocha": "^10.0.0",
    "mssql": "^8.1.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.7",
    "passport-discord": "^0.1.4",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-gitlab2": "^5.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-instagram": "^1.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-microsoft": "^1.0.0",
    "passport-slack": "^0.0.7",
    "passport-steam": "^1.0.17",
    "pg": "^8.7.3",
    "sqlite3": "^5.0.11",
    "the-log": "^3.0.1",
    "twitter-api-v2": "^1.12.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.20",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-security": "^1.5.0",
    "node-fetch": "^2.6.7",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanoff/the-api.git"
  },
  "keywords": [
    "THE",
    "API",
    "REST"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "Dimitry Ivanov <2@ivanoff.org.ua> # curl -A cv ivanoff.org.ua"
}
